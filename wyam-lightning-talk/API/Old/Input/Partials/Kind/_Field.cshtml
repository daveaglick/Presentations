@{
	IDocument containingNamespace = Model.Get<IDocument>("ContainingNamespace");
	IDocument containingType = Model.Get<IDocument>("ContainingType");
	if(containingNamespace != null 
		|| containingType != null)
	{
		<dl class="dl-horizontal">		
			@if(containingNamespace != null)
			{		
				<dt>Namespace</dt>
				<dd>@containingNamespace.Link()</dd>
			}
			
			@if(containingType != null)
			{
				<dt>Containing Type</dt>
				<dd>@containingType.Link()</dd>		
			}
		</dl>
		<hr />	
	}
}

@Html.Partial("Partials\\Section\\_Summary")
@Html.Partial("Partials\\Section\\_Syntax")
@Html.Partial("Partials\\Section\\_Value")
@Html.Partial("Partials\\Section\\_Remarks")
@Html.Partial("Partials\\Section\\_Examples")
@Html.Partial("Partials\\Section\\_SeeAlso")