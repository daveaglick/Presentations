@{
	ViewData["DocsTable"] = new DocsTable
	{
		Docs = Model.Get<IReadOnlyList<IDocument>>("Constructors")
			?.Where(x => x.Get<bool>("IsResult"))
			.OrderBy(x => x["DisplayName"])
			.ToList(),
		Title = "Constructors",
		Header = "Name",
		HasSummary = true	
	};
	@Html.Partial("Partials\\Section\\_DocsTable");
}